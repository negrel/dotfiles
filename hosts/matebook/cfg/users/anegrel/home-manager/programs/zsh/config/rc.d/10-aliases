alias df="df -h"
alias free="free -h"
alias cat="$(command -v bat || command -v cat || echo /bin/cat)"
alias ls="$(command -v lsd || command -v ls || echo /bin/ls)"
alias l="ls"
alias la="l -a"
alias ll="l -l"
alias lla="la -l"
alias du="du -sh"
alias rm="rm -i"
alias fd="fd -I"
alias ip="ip -c"
alias x="exit"
alias :q="exit"
alias :q!="exit"
alias :qa="exit"
alias :q!="exit"
alias c="clear"

# Process handling
alias psg="ps aux | grep"
alias ka9="killall -9"
alias k9="kill -9"

